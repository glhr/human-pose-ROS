<?xml version="1.0"?>
<launch>
  <arg name="cam" default="wrist" />
  <node name="openpifpaf" pkg="human_pose_ROS" type="openpifpaf_ros.py" args="--cam --realsense $(arg cam) --scale 0.3" required="true" output="screen"/>
  <node name="ros_transform" pkg="human_pose_ROS" type="ros_transform.py" required="true" output="screen"/>
  <node name="filter_skeletons" pkg="human_pose_ROS" type="filter_skeletons.py" required="true" output="screen"/>
  <node name="visualize_skeletons" pkg="human_pose_ROS" type="visualize_skeletons.py" args="" required="true" output="screen"/>


</launch>
